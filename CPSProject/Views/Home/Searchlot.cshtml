@model IEnumerable<CPSProject.Models.db.V_SHIP_BLUE_BOX_WITH_INCMG>

@{
    ViewData["Title"] = "Searchlot";
}

<h1>Searchlot</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Date1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Time1)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.LotNo)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ROHM_Model_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.QTY)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Type_Name)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.BOX)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.PDCD)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Date1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Time1)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LotNo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ROHM_Model_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.QTY)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Type_Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.BOX)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.PDCD)
            </td>
            
        </tr>
}
    </tbody>
</table>
